{
  "name": "AMQ BR Helper",
  "version": "1.0",
  "description": "Extract the list of animes you picked up from the battle royale game mode.",
  "minimum_chrome_version": "40.0",
  "manifest_version": 2,
  "browser_action": {
    "default_title": "AMQ BR Helper",
    "default_popup": "src/popup/popup.html",
    "default_icon": "images/icon128.png"
  },
  "icons": {
    "128": "images/icon128.png"
  },

  "permissions": [
    "https://animemusicquiz.com/*",
    "tabs",
    "storage"
  ]

}